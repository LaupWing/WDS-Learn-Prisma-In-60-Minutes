// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
   provider = "prisma-client-js"
}

datasource db {
   provider = "mysql"
   url      = env("DATABASE_URL")
}

model User {
   id          Int    @id @default(autoincrement())
   name        String
   preferences Json
   blob        Bytes
}

model Post {
   rating    Decimal
   createdat DateTime
   updatedAt DateTime
   id        Int      @id @default(autoincrement())
}
